{
  "name": "finish-finder",
  "version": "0.1.0",
  "private": true,
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "prisma generate && next build",
    "start": "next start",
    "postinstall": "prisma generate && husky install",
    "prepare": "husky install",
    "lint": "eslint",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "predict:all": "npx ts-node scripts/unified-ai-predictions-runner.ts",
    "pages:build": "node scripts/prepare-github-pages.js",
    "sherdog:test:local": "node scripts/test-sherdog-local.js",
    "db:migrate": "npx prisma migrate dev",
    "db:push": "npx prisma db push",
    "db:reset": "npx prisma migrate reset",
    "db:check": "npx ts-node scripts/checks/check-all-events.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.68.0",
    "@fontsource-variable/karla": "^5.2.8",
    "@fontsource-variable/roboto-condensed": "^5.2.8",
    "@headlessui/react": "^2.2.8",
    "@heroicons/react": "^2.2.0",
    "@prisma/client": "^6.16.2",
    "@sentry/nextjs": "^10.32.1",
    "@sentry/node": "^10.12.0",
    "axios": "^1.12.2",
    "cheerio": "^1.1.2",
    "csv-parser": "^3.2.0",
    "csv-writer": "^1.6.0",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.14",
    "next": "^16.1.1",
    "node-html-parser": "^7.0.1",
    "openai": "^5.21.0",
    "prisma": "^6.16.2",
    "puppeteer": "^24.22.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/coverage-v8": "^3.2.4",
    "csv-parse": "^6.1.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "jsdom": "^27.0.0",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "vitest": "^3.2.4"
  }
}
